    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "three": "^0.149.0",
    "three-obj-mtl-loader": "^1.0.3",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.133",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/element-resize-detector": "^1.1.3",
